<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.overflowitems.SettingsActivity">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_export"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/export"
        app:layout_constraintBottom_toTopOf="@+id/activity_settings_btn_export_shm"
        app:layout_constraintTop_toBottomOf="@+id/activity_settings_btn_import_wal"
        tools:layout_editor_absoluteX="32dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_export_shm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="export shm"
        app:layout_constraintBottom_toTopOf="@+id/activity_settings_btn_export_wal"
        app:layout_constraintTop_toBottomOf="@+id/activity_settings_btn_export"
        tools:layout_editor_absoluteX="32dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_export_wal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="export wal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_settings_btn_export_shm"
        tools:layout_editor_absoluteX="32dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_import"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="import data"
        app:layout_constraintBottom_toTopOf="@+id/activity_settings_btn_import_shm"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="32dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_import_shm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="import shm"
        app:layout_constraintBottom_toTopOf="@+id/activity_settings_btn_import_wal"
        app:layout_constraintTop_toBottomOf="@+id/activity_settings_btn_import"
        tools:layout_editor_absoluteX="32dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/activity_settings_btn_import_wal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="import wal"
        app:layout_constraintBottom_toTopOf="@+id/activity_settings_btn_export"
        app:layout_constraintTop_toBottomOf="@+id/activity_settings_btn_import_shm"
        tools:layout_editor_absoluteX="32dp" />

</androidx.constraintlayout.widget.ConstraintLayout>